{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "background": "#35061c",
          "foreground": "#e488b6",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " \ue5ff {{ .Path }} "
        },
        {
          "type": "git",
          "background": "#000000",
          "foreground": "#afd6c9",
          "properties": {
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": false
          },
          "style": "plain",
          "template": " \ue709 {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} "
        },
        {
          "type": "root",
          "background": "#6b6b0c",
          "foreground": "#ffffff",
          "style": "plain",
          "template": " \uf0e7 "
        },
        {
          "type": "dart",
          "background": "#0d0d0d",
          "foreground": "#06A4CE",
          "style": "plain",
          "template": " \uE798 {{ .Full }} "
        },
        {
          "type": "java",
          "background": "#350303",
          "foreground": "#e67b90",
          "style": "plain",
          "template": " \ue738 {{ .Full }} "
        },
        // {
        //   "type": "time",
        //   "background": "#35061c",
        //   "foreground": "#e488b6",
        //   "style": "plain"
        // },
        {
          "type": "exit",
          "background": "#095035",
          "background_templates": ["{{ if gt .Code 0 }}#b3153a{{ end }}"],
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \uf188 "
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
